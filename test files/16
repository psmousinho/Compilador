program teste; {programa exemplo}
var
    valor1: integer;
    valor2: real;

procedure olha_o_void(n: integer);
    var
        x: integer;
    begin
        x := 10;
    end;

function fatorial(n, n1, n2: integer; n3: real): integer;
    var
        x: integer;
        b: boolean;
    begin
        x := (-10*5) + 5*6;
        b := x < fatorial(10,1,2,3);
        olha_o_void(5);
        fatorial(1, 2, fatorial(fatorial(1, 2, 3, 4), 2, 3, 4), 4);
        fatorial := x;
    end;
begin
    valor1 := 10;
end.
